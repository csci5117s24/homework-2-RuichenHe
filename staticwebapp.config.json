{
    "routes": [
        {
            "route": "/todos",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/todos/*",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/todo/*",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/done",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/done/*",
            "allowedRoles": ["authenticated"]
        }
      ],

    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
    }, 
    "responseOverrides":{
        "401": {
            "statusCode": 302,
            "redirect": "/"
        }
    }
  }